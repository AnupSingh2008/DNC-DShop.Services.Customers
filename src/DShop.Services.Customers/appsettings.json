    "pingEndpoint": "ping",
    "pingInterval": 5,
    "removeAfterInterval": 10,
    "requestRetries": 3
  },
  "elk": {
    "enabled": false,
    "url": "http://localhost:9200",
    "indexFormat": "customers-service-{0:yyyy.MM.dd}",
    "basicAuthEnabled": false,
    "username": "user",
    "password": "secret"
  },
	"metrics": {
    "enabled": true,
    "prometheusEnabled": true,
    "influxUrl": "http://localhost:8086",
    "database": "dshop",
    "env": "local",
    "interval": 5
  },
  "mongo": {
    "connectionString": "mongodb://localhost:27017",
    "database": "dshop-customers-service",
    "seed": false
  },
	"rawRabbit": {
		"username": "guest",
		"password": "guest",
		"virtualHost": "/",
		"port": 5672,
		"hostnames": ["localhost"],
		"requestTimeout": "00:00:10",
		"publishConfirmTimeout": "00:00:01",
		"recoveryInterval": "00:00:10",
		"persistentDeliveryMode": true,
		"autoCloseConnection": true,
		"automaticRecovery": true,
		"topologyRecovery": true,
		"exchange": {
				"durable": true,
				"autoDelete": false,
				"type": "Topic"
		},
		"queue": {
				"autoDelete": false,
				"durable": true,
				"exclusive": false
		}
	},
  "restEase": {
    "useConsul": false,
    "services": [
      {
        "name": "products-service",
        "host": "localhost",
        "scheme": "http",
        "port": "5006"
      }
    ]
  },
  "serilog": {
    "consoleEnabled": true,
    "level": "information"
  },
  "seq": {
    "enabled": true,
    "url": "http://seq:5341",
    "token": "secret"
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "v1",
    "title": "Customers Service",
    "version": "v1",
    "routePrefix": "docs",
    "includeSecurity": false
  },
  "vault": {
    "enabled": false,
    "url": "http://localhost:8200",
