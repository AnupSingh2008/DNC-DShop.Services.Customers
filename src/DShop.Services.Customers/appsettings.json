{
	"consul": {
    "url": "http://localhost:8500",
    "service": "customers-service",
    "address": "localhost",
    "port": "5001",
    "pingEnabled": false,
    "pingEndpoint": "ping",
    "pingInterval": 5,
    "removeAfterInterval": 10,
    "requestRetries": 3
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      }
    }
	},
	"metrics": {
    "database": "dshop",
    "uri": "http://localhost:8086",
    "env": "local",
    "interval": 5,
    "enabled": true
  },
  "mongo": {
    "connectionString": "mongodb://localhost:27017",
    "database": "dshop-customers-service",
    "seed": false
  },
	"rawRabbit": {
		"username": "guest",
		"password": "guest",
		"virtualHost": "/",
		"port": 5672,
		"hostnames": ["localhost"],
		"requestTimeout": "00:00:10",
		"publishConfirmTimeout": "00:00:01",
		"recoveryInterval": "00:00:10",
		"persistentDeliveryMode": true,
		"autoCloseConnection": true,
		"automaticRecovery": true,
		"topologyRecovery": true,
		"exchange": {
				"durable": true,
				"autoDelete": false,
				"type": "Topic"
		},
		"queue": {
				"autoDelete": false,
				"durable": true,
				"exclusive": false
		}
	},
  "restEase": {
    "useConsul": false,
    "services": [
      {
        "name": "products-service",
        "host": "localhost",
        "scheme": "http",
        "port": "5006"
      }
    ]
  },
  "vault": {
    "enabled": false,
    "url": "http://localhost:8200",
    "key": "customers-service/options",
    "authType": "token",
    "token": "secret",
    "username": "user",
    "password": "secret"
  }
}